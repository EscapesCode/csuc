<%- include('partials/headerAdmin') %>

    <div class="container mt-5">
        <div class="row">
            <div class="col">
                <h1>Tambah Data Achievements</h1>
            </div>
        </div>
        <div class="row">
            <div class="col-md-5">
                <form action="/admin/acv/tambah" method="post">
                    <div class="form-floating mb-3">
                        <input type="text" name="rank" class="form-control" id="rank" placeholder="Rank Place">
                        <label for="rank">Rank Place</label>
                    </div>
                    <div class="form-floating mb-3">
                        <input type="text" name="competition" class="form-control" id="Competition"
                            placeholder="Competition">
                        <label for="Competition">Competition</label>
                    </div>
                    <div class="form-floating mb-3">
                        <input type="text" name="date" class="form-control" id="Date" placeholder="Date">
                        <label for="Date">Date</label>
                    </div>
                    <div class="form-floating mb-3">
                        <button type="submit" class="btn btn-primary">Tambah Data</button>
                    </div>
                </form>
            </div>
        </div>
    </div>

    <%- include('partials/footerAdmin') %>